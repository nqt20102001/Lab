@model Lab.Application.DTOs.BookCatalogDTO

<h2>Book Catalog Details</h2>

<div>
    <h4>Book: @Model.Book?.Title</h4>
    <h4>Catalog: @Model.Catalog?.Title</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Book ID</dt>
        <dd class="col-sm-10">@Model.BookId</dd>

        <dt class="col-sm-2">Catalog ID</dt>
        <dd class="col-sm-10">@Model.CatalogId</dd>
    </dl>

    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
